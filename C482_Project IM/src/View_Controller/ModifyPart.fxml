<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="541.0" prefWidth="527.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ModifyPartController">
   <children>

      <Label layoutX="32.0" layoutY="45.0" text="Modify Part">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="inHouseRadio" layoutX="154.0" layoutY="52.0" mnemonicParsing="false" onAction="#selectInHouse" prefHeight="19.0" prefWidth="89.0" text="In-House">
